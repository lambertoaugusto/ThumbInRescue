    else if(t == 'mina'){
        temaAtual = irandom(2);
    }
    else if(t == 'vulcao'){
        temaAtual = irandom(3);
    }
    if(temaAnterior != temaAtual){
        global.mudouTema = true;
        global.modificarObjetos = false;
    }
    //global.transicao = true;
    instance_create(LimiteEsq,global.alturaTela,obj_background0);      
}

